# Easter calculation
Easter <- function(year=1971){
d <- ((year %% 19) * 18.37 -6) %% 29
s <- as.numeric(as.Date(paste(year,3,d,sep="-"))) + 5
s <- (s %/% 7 * 7 + 24)
return(format(as.Date(s,origin="1970-01-01"),"%d-%m-%Y"))
}
Easter(2019)
